{
    "watch": [
        "./server.js",
        "./app/",
        "./src/"
    ],
    "ext": "js, ts, json, njk, html, css, scss, yaml",
    "ignore": [
        "dist/",
        "node_modules",
        ".git"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "execMap": {
        "js": "node"
    }
}