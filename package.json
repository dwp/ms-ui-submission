{
  "name": "ms-ui-submission",
  "version": "5.3.0",
  "engines": {
    "node": "^14.0.0 || ^16.0.0 || ^18.0.0",
    "npm": "^8.0.0"
  },
  "type": "module",
  "description": "Customer-facing web application to manage submissions for contributions-based Employment and Support Allowance",
  "main": "./app/server.js",
  "files": [
    "app/**/*",
    "gulpfile.js"
  ],
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-import-assertions"
    ]
  },
  "scripts": {
    "start": "node ./app/server.js",
    "start:nodemon": "nodemon ./app/server.js",
    "start:dev": "env $(cat .env.dev | grep -v ^# | xargs) npm run start:nodemon",
    "start:docker": "docker-compose up -d --build localstack localstack-init redis-cluster stub app-local",
    "stop:docker": "docker-compose down",
    "test": "npm run test:unit",
    "test:unit": "mocha \"test/unit/**/*.js\"",
    "test:smoke": "docker-compose up -d --build localstack localstack-init redis-cluster stub selenium app-local && docker-compose up --exit-code-from smoke_test smoke_test && docker-compose down",
    "test:journey": "docker-compose up -d --build localstack localstack-init redis-cluster stub selenium app-local && docker-compose up --exit-code-from journey_test journey_test && docker-compose down",
    "test:axe": "docker-compose up -d --build localstack localstack-init redis-cluster stub selenium app-local && docker-compose up --exit-code-from axe_test axe_test && docker-compose down",
    "quality:lint": "eslint -c node_modules/@dwp/eslint-config-base/.eslintrc.cjs .",
    "quality:lintfix": "eslint -c node_modules/@dwp/eslint-config-base/.eslintrc.cjs --fix .",
    "quality:coverage": "nyc --reporter=lcov npm test",
    "fill": "export PATH=$PATH:$(pwd)/node_modules/chromedriver/bin && node tools/complete-form.js",
    "fill:all": "export SUBMITCLAIM=true && node tools/call-complete-form.js",
    "package:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "errorpages": "./node_modules/.bin/gulp",
    "build": "./node_modules/.bin/gulp timeout"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.nonprod.dwpcloud.uk:health/ns-esa/ms-ui-submission.git"
  },
  "keywords": [],
  "author": "DWP Digital Health",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-sqs": "3.598.0",
    "@dwp/dwp-cryptoservice": "2.2.5-release.531847260",
    "@dwp/dwp-winston-format": "1.3.0-release.707873088",
    "@dwp/govuk-casa": "8.12.0",
    "connect-redis": "5.2.0",
    "cookie-parser": "1.4.6",
    "csrf-sync": "^4.0.3",
    "csurf": "1.11.0",
    "debug": "4.3.5",
    "deepmerge": "4.3.1",
    "del": "6.1.1",
    "express": "4.19.2",
    "express-session": "1.18.0",
    "google-libphonenumber": "3.2.34",
    "govuk-frontend": "4.6.0",
    "gulp": "^4.0.2",
    "gulp-base64-v2": "1.0.4",
    "gulp-inline": "0.1.3",
    "gulp-inline-assets": "0.1.1",
    "gulp-nunjucks-render": "2.2.3",
    "gulp-replace": "1.1.4",
    "gulp-uglify": "3.0.2",
    "hashids": "2.3.0",
    "helmet": "7.1.0",
    "i18next": "23.11.5",
    "i18next-http-middleware": "3.6.0",
    "ioredis": "5.4.1",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "lodash.isequal": "4.5.0",
    "lodash.merge": "4.6.2",
    "luxon": "3.4.4",
    "notifications-node-client": "4.9.0",
    "nunjucks": "3.2.4",
    "path-to-regexp": "6.2.2",
    "punycode": "2.3.1",
    "request-promise-native": "1.0.9",
    "session-file-store": "1.5.0",
    "uuid": "10.0.0",
    "validator": "13.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/eslint-parser": "7.23.3",
    "@babel/plugin-syntax-import-assertions": "^7.23.3",
    "@babel/preset-env": "7.21.5",
    "@ckeditor/jsdoc-plugins": "37.0.1",
    "@commitlint/config-conventional": "17.6.3",
    "@dwp/casa-spiderplan": "3.0.0",
    "@dwp/casa-spiderplan-a11y-plugin": "0.1.12",
    "@dwp/casa-spiderplan-zap-plugin": "0.1.8",
    "@dwp/eslint-config-base": "7.0.0",
    "@types/express": "4.17.17",
    "@types/node": "18.16.9",
    "@types/nunjucks": "3.2.2",
    "aws-sdk-client-mock": "3.0.0",
    "babel-plugin-rewire": "^1.2.0",
    "body-parser": "^1.20.2",
    "c8": "7.13.0",
    "chai": "4.3.10",
    "chai-as-promised": "7.1.1",
    "chai-subset": "1.6.0",
    "cheerio": "1.0.0-rc.12",
    "chromedriver": "103.0.0",
    "commitlint": "17.6.3",
    "docdash": "2.0.1",
    "eslint": "8.40.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "faker": "5.5.3",
    "fast-check": "3.8.1",
    "i18n": "0.15.1",
    "jsdoc": "4.0.2",
    "mocha": "10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "15.1.0",
    "selenium-webdriver": "4.8.1",
    "sinon": "15.0.4",
    "sinon-chai": "3.7.0",
    "supertest": "^6.3.3"
  },
  "overrides": {
    "request": {
      "qs": "6.11.1"
    },
    "@babel/traverse": "7.24.1",
    "glob-parent": "6.0.2",
    "nth-check": "2.0.1"
  }
}
